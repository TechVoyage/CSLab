{
    "root": ".",
    "author": "Hengyuan",
    "title": "YUAN HANG CS·Lab",
    "language": "zh-hans",
    "description": "This is a Pages for YUAN HANG CS·Lab",
    "direction": "ltr",
    "gitbook": ">=3.2.3",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "advanced-emoji",
        "search-plus",
        "search-pro",
        "github",
        "splitter",
        "anchor-navigation-ex",
        "expandable-chapters",
        "code",
        "alerts",
        "insert-logo",
        "flexible-alerts",
        "disqus",
        "theme-comscore",
        "theme-fexa",
        "favicon-absolute"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/rattlesnakey"
        },
        "insert-logo": {
            "url": "https://s2.loli.net/2021/12/04/uhF9o6eHBqptLfJ.jpg",
            "style": "background: none; max-height: 100px; min-height: 30px"
        },
        "flexible-alerts": {
            "style": "callout",
            "comment": {
                "label": "Comment",
                "icon": "fa fa-comments",
                "className": "info"
            }
        },
        "disqus": {
            "shortName": "cslab-1"
        },
        "theme-fexa":{
            "search-placeholder":"输入关键字搜索",
            "logo":"local_assets/favicon.ico",
            "favicon": "local_assets/favicon.ico"
        },
        "favicon-absolute":{
            "favicon": "local_assets/favicon.ico",
            "appleTouchIconPrecomposed152": "local_assets/favicon.ico"
        }
    }}

